<%- include("partials/header.ejs") %>

<body class="whole">
    <div class="viewBlog">
        <% let blog=blogs[index]%>
        <div class="blogcontent">
            <h1> <%=blog.title %></h1>
            <h5> <%=blog.date %></h5>
            <h3><%=blog.author %></h3>
            <%- blog.content%>
        </div>
        <div class="btns back">
        <form action="/allBlogs" method="get">
            <input type="submit" value="Back">
        </form>
        </div>
    </div>
</body>
